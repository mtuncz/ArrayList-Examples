public class Exercise {
    public static void main(String[] args) {
        List<String> c1 = new ArrayList<String>();
        c1.add("Red");
        c1.add("Green");
        c1.add("Black");
        c1.add("White");
        c1.add("Pink");

        System.out.println(c1);

        String temp = c1.get(0);
        c1.set(0, c1.get(2));
        c1.set(2, temp);
        System.out.println(c1);

        Collections.swap(c1, 0,2);
        System.out.println(c1);


    }
}
