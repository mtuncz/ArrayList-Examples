public class Merge
{
  public static void main(String[] args) throws IOException
  {
  ArrayList<Integer> list1 = new ArrayList<Integer>(); // create the list
  Collections.addAll(list1, 1, 5, 6, 11, 3, 15, 7, 8); // fill the list
  
  ArrayList<Integer> list2 = new ArrayList<Integer>(); 
  Collections.addAll(list2, 1, 8, 6, 21, 53, 5, 67, 18);
  
  ArrayList<Integer> result = new ArrayList<Integer>(); // this will be our final merged list 
  
  result.addAll(list1); // adding all values from list1 to result list
  result.addAll(list2); // adding all values from list2 to result list 
  
  for(Integer x : result){ // looping over each element in the result list and printing it. to the console.
    System.out.println(x);
  }
 }
}
